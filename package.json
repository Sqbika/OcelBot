{
    "name": "OcelBot",
    "version": "0.0.1",
    "description": "I dunno",
    "main": "main.js",
    "dependencies": {
        "body-parser": "^1.18.2",
        "chalk": "^1.1.3",
        "cors": "^2.8.4",
        "discord.js": "^11.2.1",
        "express": "^4.16.2",
        "fs-jetpack": "^0.10.5",
        "googleapis": "^20.1.0",
        "he": "^1.1.1",
        "humanize-duration": "^3.10.1",
        "mail-listener2": "^0.3.1",
        "moment": "^2.17.1",
        "mongodb": "^2.2.26",
        "pg": "^7.1.2",
        "pg-hstore": "^2.3.2",
        "photo-collage": "^1.0.0",
        "request-promise-native": "^1.0.4",
        "sequelize": "^4.5.0",
        "socket.io": "^2.0.4",
        "twit": "^2.2.5",
        "uuid": "^3.1.0",
        "winston": "^2.3.1",
        "winston-daily-rotate-file": "^1.4.6"
    },
    "devDependencies": {
        "eslint": "^4.12.1",
        "eslint-config-xo": "^0.19.0",
        "eslint-plugin-ava": "^4.3.0",
        "eslint-plugin-import": "^2.8.0",
        "eslint-plugin-no-use-extend-native": "^0.3.12",
        "eslint-plugin-promise": "^3.6.0",
        "eslint-plugin-unicorn": "^3.0.0",
        "xo": "^0.18.2"
    },
    "scripts": {
        "test": "xo"
    },
    "xo": {
        "rules": {
            "no-unused-expressions": [
                "error",
                {
                    "allowTernary": true,
                    "allowShortCircuit": true
                }
            ],
            "camelcase": [
                "error",
                {
                    "properties": "never"
                }
            ],
            "no-unassigned-import": [
                "off",
                {}
            ],
            "new-cap": [
                "error",
                {
                    "capIsNew": false
                }
            ],
            "no-return-assign": [
                "off",
                {}
            ],
            "complexity": [
                "error",
                50
            ],
            "no-await-in-loop": [
                "off",
                {}
            ]
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/artemisbot/OcelBot.git"
    },
    "keywords": [
        "discord",
        "discord.js"
    ],
    "author": "Matt C [matt@artemisbot.uk]",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/artemisbot/OcelBot/issues"
    },
    "homepage": "https://github.com/artemisbot/OcelBot#readme"
}
