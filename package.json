{
    "name": "OcelBot",
    "version": "0.0.1",
    "description": "I dunno",
    "main": "main.js",
    "dependencies": {
        "body-parser": "^1.18.2",
        "canvas": "^1.6.9",
        "chalk": "^2.3.0",
        "child-process-promise": "^2.2.1",
        "cors": "^2.8.4",
        "css-select": "^1.3.0-rc0",
        "discord.js": "^11.2.1",
        "edit-google-spreadsheet": "^0.3.0",
        "express": "^4.16.2",
        "fs-jetpack": "^1.2.0",
        "googleapis": "^23.0.0",
        "he": "^1.1.1",
        "htmlparser2": "^3.9.2",
        "humanize-duration": "^3.12.1",
        "mail-listener2": "^0.3.1",
        "moment": "^2.20.1",
        "pg": "^7.4.1",
        "photo-collage": "^1.0.0",
        "request": "^2.83.0",
        "request-promise-native": "^1.0.4",
        "sequelize": "^4.29.0",
        "snekfetch": "^3.6.1",
        "strftime": "^0.10.0",
        "twit": "^2.2.9",
        "twitter-media": "^0.2.0",
        "uuid": "^3.1.0",
        "webshot": "git+https://github.com/adespresso/node-webshot.git#pass-zoom-factor-to-phantom",
        "winston": "^2.4.0",
        "winston-daily-rotate-file": "^1.7.2"
    },
    "devDependencies": {
        "eslint": "^4.15.0",
        "eslint-config-xo": "^0.19.0",
        "eslint-plugin-ava": "^4.4.0",
        "eslint-plugin-import": "^2.8.0",
        "eslint-plugin-no-use-extend-native": "^0.3.12",
        "eslint-plugin-promise": "^3.6.0",
        "eslint-plugin-unicorn": "^3.0.1",
        "xo": "^0.18.2"
    },
    "scripts": {
        "test": "xo"
    },
    "xo": {
        "rules": {
            "no-unused-expressions": [
                "error",
                {
                    "allowTernary": true,
                    "allowShortCircuit": true
                }
            ],
            "camelcase": [
                "error",
                {
                    "properties": "never"
                }
            ],
            "no-unassigned-import": [
                "off",
                {}
            ],
            "new-cap": [
                "error",
                {
                    "capIsNew": false
                }
            ],
            "no-return-assign": [
                "off",
                {}
            ],
            "complexity": [
                "error",
                50
            ],
            "no-await-in-loop": [
                "off",
                {}
            ],
            "max-depth": [
                "error",
                {
                    "max": 7
                }
            ]
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/artemisbot/OcelBot.git"
    },
    "keywords": [
        "discord",
        "discord.js"
    ],
    "author": "Matt C [matt@artemisbot.uk]",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/artemisbot/OcelBot/issues"
    },
    "homepage": "https://github.com/artemisbot/OcelBot#readme"
}
